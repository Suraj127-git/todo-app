{
  "expo": {
    "name": "todo-app",
    "slug": "remindernotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/57525483-61ad-47be-9e08-0960ed2c178d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.surajshettyexpo.remindernotes",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "softwareKeyboardLayoutMode": "pan",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      [
        "@config-plugins/react-native-android-widget",
        {
          "widgets": [
            {
              "name": "TodoWidget",
              "label": "Todo Widget",
              "minWidth": "200dp",
              "minHeight": "100dp",
              "updatePeriodMillis": 1800000,
              "previewImage": "./assets/widget-preview.png",
              "targets": ["android"]
            }
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "57525483-61ad-47be-9e08-0960ed2c178d"
      }
    },
    "owner": "surajshettyexpo"
  }
}